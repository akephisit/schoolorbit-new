# Environment Variables for SvelteKit
# Copy this file to .env for local development
# Public variables (exposed to client) must start with PUBLIC_

# Backend Admin API (accessible from client)
PUBLIC_API_URL=http://localhost:8080

# Backend School API (for server-side API routes only)
# Used by /api/migration/* endpoints to communicate with backend-school service
BACKEND_SCHOOL_URL=http://localhost:8081
INTERNAL_API_SECRET=your-internal-secret-change-this-in-production
